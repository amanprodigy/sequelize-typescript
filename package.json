{
    "name": "nodejs-typescript-sequelize-mysql-rest-api",
    "version": "1.0.0",
    "description": "Tutorial on Building REST API with Nodejs Express Sequelize",
    "main": "src/index.ts",
    "scripts": {
        "prestart": "tsc",
        "start": "cross-env BUILD_ENV=development NODE_ENV=development nodemon out/index.js",
        "debug": "npm run prestart && cross-env BUILD_ENV=development NODE_ENV=development node-inspect out/index.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "db:create": "sequelize db:create",
        "db:migrate": "sequelize db:migrate",
        "db:migrate:status": "sequelize db:migrate:status",
        "db:migrate:undo": "sequelize db:migrate:undo",
        "db:migrate:undo:all": "sequelize db:migrate:undo:all"
    },
    "dependencies": {
        "@types/express-boom": "^3.0.0",
        "bcrypt": "^4.0.1",
        "body-parser": "^1.19.0",
        "express": "^4.17.1",
        "express-boom": "^3.0.0",
        "lodash": "^4.17.15",
        "module-alias": "^2.2.2",
        "morgan": "^1.10.0",
        "mysql": "^2.18.1",
        "mysql2": "^2.1.0",
        "reflect-metadata": "^0.1.13",
        "sequelize": "^5.21.7",
        "sequelize-typescript": "^1.1.0",
        "umzug": "^2.3.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/bluebird": "^3.5.30",
        "@types/express": "^4.17.6",
        "@types/lodash": "^4.14.150",
        "@types/morgan": "^1.9.0",
        "@types/node": "^13.13.5",
        "@types/nodemon": "^1.19.0",
        "@types/sequelize": "^4.28.8",
        "@types/umzug": "^2.2.3",
        "@types/validator": "^13.0.0",
        "cross-env": "^7.0.2",
        "nodemon": "^2.0.3",
        "sequelize-cli": "^5.5.1",
        "ts-node": "^8.10.1",
        "typescript": "^3.8.3"
    },
    "_moduleAliases": {
        "@app": "out",
        "@controllers": "out/controllers",
        "@db": "out/db",
        "@dao": "out/dao",
        "@routes": "out/routes"
    },
    "author": "",
    "license": "ISC"
}
